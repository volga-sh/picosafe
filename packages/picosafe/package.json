{
	"name": "@volga/picosafe",
	"version": "0.0.1",
	"description": "A TypeScript SDK for Safe Smart Account contracts",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"prepare": "npm run build",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test": "vitest run",
		"test:anvil-verbose": "ANVIL_VERBOSE=true vitest run",
		"typecheck": "tsc --noEmit",
		"coverage": "vitest run --coverage"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "^22.13.10",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"@volga/anvil-manager": "*",
		"@volga/safe-genesis": "*",
		"rollup": "^4.46.2",
		"tsup": "^8.5.0",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"viem": "^2.33.2"
	}
}
