{
	"name": "@volga/picosafe",
	"version": "0.0.1",
	"description": "A TypeScript SDK for Safe Smart Account contracts",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"prepare": "npm run build",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test": "vitest run",
		"test:anvil-verbose": "ANVIL_VERBOSE=true vitest run",
		"typecheck": "tsc --noEmit",
		"coverage": "vitest run --coverage",
		"check": "biome check",
		"check:write": "biome check --write"
	},
	"keywords": [],
	"author": "",
	"license": "Unlicense",
	"devDependencies": {
		"@types/node": "^24.10.0",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/ui": "^4.0.18",
		"@volga/anvil-manager": "*",
		"@volga/safe-genesis": "*",
		"rollup": "^4.57.1",
		"tsup": "^8.5.1",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"ox": "^0.12.1"
	}
}
