{
	"name": "@volga/anvil-manager",
	"version": "0.1.0",
	"description": "Anvil process management for tests and examples",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"prepare": "npm run build",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"cleanup-orphans": "tsx scripts/cleanup-orphans.ts",
		"cleanup-orphans:dry": "tsx scripts/cleanup-orphans.ts --dry-run"
	},
	"devDependencies": {
		"@types/node": "^22.10.5",
		"tsup": "^8.5.0",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": ">=22"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/volga-sh/picosafe.git",
		"directory": "packages/anvil-manager"
	},
	"keywords": [
		"anvil",
		"ethereum",
		"foundry",
		"testing",
		"blockchain"
	],
	"license": "Unlicense"
}
